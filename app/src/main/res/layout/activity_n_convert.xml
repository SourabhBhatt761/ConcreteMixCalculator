<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:background="#00b8d4" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginBottom="50dp">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <androidx.cardview.widget.CardView android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="5dp" android:layout_marginTop="15dp" app:cardElevation="20dp">
                <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                    <TextView android:textSize="16dp" android:textColor="#000000" android:gravity="center_horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Total Volume" android:layout_weight="1"/>
                    <EditText android:textSize="16dp" android:textColor="#000000" android:gravity="center_horizontal" android:id="@+id/ivolume" android:layout_width="0dp" android:layout_height="wrap_content" android:text="1" android:layout_weight="1" android:inputType="numberDecimal"/>
                    <TextView android:textSize="16dp" android:textColor="#000000" android:gravity="center_horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:text="m^3" android:layout_weight="1"/>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
            <Button android:textColor="#01608c" android:layout_gravity="center_horizontal" android:id="@+id/nget" android:layout_width="250dp" android:layout_height="wrap_content" android:text="CALCULATE"/>
            <androidx.cardview.widget.CardView android:layout_gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="5dp" app:cardCornerRadius="5dp" app:cardElevation="20dp" app:contentPadding="10dp">
                <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Cement" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_cement" android:layout_width="0dp" android:layout_height="wrap_content" android:text="cement" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="GGBS" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_ggbs" android:layout_width="0dp" android:layout_height="wrap_content" android:text="ggbs" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Flyash" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_flyash" android:layout_width="0dp" android:layout_height="wrap_content" android:text="flyash" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Metakoaline" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_matakaoline" android:layout_width="0dp" android:layout_height="wrap_content" android:text="matakaoline" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Rice husk ash" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_rice" android:layout_width="0dp" android:layout_height="wrap_content" android:text="rice" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Silica Fume" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_silica" android:layout_width="0dp" android:layout_height="wrap_content" android:text="silica" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Water" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_water" android:layout_width="0dp" android:layout_height="wrap_content" android:text="water" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Fine Aggregate" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_fine" android:layout_width="0dp" android:layout_height="wrap_content" android:text="fine" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Coarse Aggregate" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_coarse" android:layout_width="0dp" android:layout_height="wrap_content" android:text="coarse" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:weightSum="3">
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="Chemical Admixture" android:layout_weight="1"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:id="@+id/con_chemical" android:layout_width="0dp" android:layout_height="wrap_content" android:text="chemical" android:layout_weight="1" android:textAlignment="center"/>
                        <TextView android:textSize="16dp" android:textColor="#000000" android:layout_width="0dp" android:layout_height="wrap_content" android:text="kg" android:layout_weight="1" android:textAlignment="center"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/banner5_container" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_alignParentBottom="true" app:layout_constraintBottom_toBottomOf="parent"/>
</RelativeLayout>
